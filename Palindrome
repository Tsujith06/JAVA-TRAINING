import java.util.*;
class Main {
    public static void main(String[] args) {
        int num=181;
        int n=num,r=0,l;
        while(n!=0){
            l=n%10;
            r=r*10+l;
            n=n/10;
        }
        if(r==num){
            System.out.print(num+" "+"is an palindrome");
        }
        else{
            System.out.print(num+" "+"is not an palindrome");
        }
    }
}
