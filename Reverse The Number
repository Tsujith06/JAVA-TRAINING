import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int r=0;
        while(n!=0){
            int l=n%10;
            r=r*10+l;
            n=n/10;
        }
        System.out.print(r);
    }
}
