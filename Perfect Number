import java.util.*;
class Main {
    public static void main(String[] args) {
        int num=28,s=0;
        for(int i=1;i<=num/2;i++){
            if(num%i==0){
                s+=i;
            }
        }
        if(s==num){
            System.out.print(num+" is an perfect number");
        }
    }
}
