import java.util.*;
class Main {
    public static void main(String[] args) {
        int num=7 ,isprime=1;
        for(int i=2;i<num;i++){
            if(num%i==0){
                isprime=0;
                break;
            }
        }
        if(isprime==1){
            System.out.print(num+" "+"is an prime number");
        }
        else{
            System.out.print(num+" "+"is not an prime number");
        }
    }
}
