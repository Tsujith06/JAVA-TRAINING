import java.util.*;
class Main {
    public static void main(String[] args) {
        int n=5,a=65;
        for(int r=1;r<=n;r++){
            for(int j=1;j<=n-r+1;j++){
                char c=(char) a;
                System.out.print(c+" ");
                a=a+1;
            }
            a=65;
            a=a+r;
            System.out.println();
        }
    }
}
