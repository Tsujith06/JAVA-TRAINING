import java.util.*;
class Main {
    public static void main(String[] args) {
        String s="i.like.this.program.very.much";
        String w[]=s.split("\\.");
        for(int i=w.length-1;i>=0;i--){
            System.out.print(w[i]);
            if(i!=0){
                System.out.print(".");
            }
        }
        
    }
}
