import java.util.*;
class Main {
    public static void main(String[] args) {
        int n=20,p=1,ai=0;
        System.out.print(2+" ");
        for(int i=3;i<n;i++){
            for(int j=2;j<i;j++){
                if(i%j==0){
                    p=0;
                    break;
                }
            }
            if(p==1){
                ai+=1;
                if(ai%2==0){
                System.out.print(i+" ");
                }
            }
            p=1;
        }
    }
}
